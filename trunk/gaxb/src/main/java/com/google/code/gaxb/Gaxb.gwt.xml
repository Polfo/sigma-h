    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.xml.XML"/>
    <inherits name="com.google.code.gaxb.Annotations"/>

    <generate-with class="com.google.code.gaxb.rebind.XmlBindingGenerator">
