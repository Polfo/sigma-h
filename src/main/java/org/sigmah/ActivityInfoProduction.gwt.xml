<module rename-to="ActivityInfo">
    <inherits name="org.sigmah.ActivityInfo"/>

    <extend-property name="locale" values="en,fr"/>
    <set-property-fallback name="locale" value="en"/>

    <!-- Turn all logging off  -->
    <extend-property name="log_level" values="OFF" />
    <set-property-fallback name="log_level" value="OFF"/>
    <set-property name="log_DivLogger" value="DISABLED" />

    <entry-point class="org.sigmah.client.ActivityInfoEntryPoint"/>
</module>

